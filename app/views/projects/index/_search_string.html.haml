= text_field_tag :search_string, @search.original_search_string, placeholder: 'search projects'

:javascript
  $(function() {

    console.log('wiring up keyup');

    $("form#faceted-search #search_string").bind('keyup', $.debounce( 400, function() {
      console.log('fire');
      $("form#faceted-search").submit();
    }));
  });